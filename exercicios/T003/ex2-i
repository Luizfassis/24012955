#include <stdio.h>
#include <stdlib.h>
#include <math.h>
int main(){
double a,b,c,r1,r2,delta;
    printf("Digite o coeficiente a: ");
    scanf("%lf",&a);
    printf("Digite o coeficiente b: ");
    scanf("%lf",&b);
    printf("Digite o coeficiente c: ");
    scanf("%lf",&c);
    delta = ((b*b)-4*a*c);
    r1 = ((-b+(sqrt(delta)))/(a*2));
    r2 = ((-b-(sqrt(delta)))/(a*2));
    printf("Sua raiz 1 e %.1lf\n",r1);
    printf("Sua raiz 2 e %.1lf\n",r2);
    return 0;
}
